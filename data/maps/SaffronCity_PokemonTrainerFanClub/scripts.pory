	textcolor 1
	applymovement LOCALID_BATTLE_GIRL, Movement_WalkInPlaceFastestDown
	waitmovement 0
	playse SE_PIN
	applymovement LOCALID_BATTLE_GIRL, Movement_ExclamationMark
